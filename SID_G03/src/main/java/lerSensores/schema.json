{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Sensor",
    "description": "Medicoes dos sensores",
    "type": "object",
    "properties": {
        "tmp":{
            "type": "string"
        },
        "hum":{
            "type":"string"

        },
        "dat":{
            "type":"string",
            "pattern":"(^(((0[1-9]|1[0-9]|2[0-8])/(0[1-9]|1[012]))|((29|30|31)/(0[13578]|1[02]))|((29|30)/(0[4,6,9]|11)))/(19|[2-9][0-9])[0-9][0-9]$)|(^29/02/(19|[2-9][0-9])(00|04|08|12|16|20|24|28|32|36|40|44|48|52|56|60|64|68|72|76|80|84|88|92|96)$)"
        },
        "tim":{
            "type":"string",
            "pattern":"^([01][0-9]|2[0123]):([012345][0-9]):([012345][0-9])"
        },
        "cell":{
            "type":"string"
        },
        "mov":{
            "type":"string"
        },
        "sens":{
            "type":"string"
        }
    },
    "required": ["dat", "tim"]
}


